{
    "plugins": [
        "plugins/markdown"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {"outputSourceFiles": false},
        "methodHeadingReturns": false,
        "systemName"            : "NiftyMenu",
        "copyright"             :  "&copy; Brett Terpstra 2020",
        "navType"               : "inline",
        "theme"                 : "spacelab",
        "linenums"              : false,
        "collapseSymbols"       : false,
        "inverseNav"            : true,
        "outputSourceFiles"     : false,
        "outputSourcePath"      : false,
        "syntaxTheme"           : "default",
        "search"                : true
    }
}
